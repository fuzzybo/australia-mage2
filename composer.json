{
    "name": "fuzzybo/australia-mage2",
    "description": "Magento extension that provides functionality for Australian stores.",
    "type": "magento2-module",
    "license": "OSL-3.0",
    "require": {
        "php": "~5.6.0|~7.0.8|~7.1.0|~7.2.0|~7.3.0|~7.4.0|~8.0.0|~8.1.0",
        "fuzzybo/auspost-api-php": "dev-main",
        "fontis/bpay-ref-generator": "^1.0.0|^2.0.0",
        "fuzzybo/customergroupaccess-mage2": "dev-master",
        "magento/framework": "100.*|101.*|102.*|103.*",
        "magento/module-checkout": "100.*|101.*|102.*|103.*",
        "magento/module-directory": "100.*|101.*|102.*|103.*",
        "magento/module-payment": "100.*|101.*|102.*|103.*",
        "magento/module-store": "100.*|101.*|102.*|103.*",
        "magento/module-shipping": "100.*|101.*|102.*|103.*"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Fontis\\Australia\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
		{
            "type": "github",
            "url": "https://github.com/fuzzybo/customergroupaccess-mage2"
        },
		{
            "type": "github",
            "url": "https://github.com/fuzzybo/auspost-api-php"
        },
		{
            "type": "github",
            "url": "https://github.com/fuzzybo/guzzle"
        }
    ]
}
